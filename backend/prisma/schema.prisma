generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model Fukuoka_land_info {
  id    Int     @id @default(autoincrement())
  location_name     String
  address          String
  record_date    String
  geometry       Json
}



model Comment {
  id        Int      @id @default(autoincrement())
  lat       Float    // 緯度
  lng       Float    // 経度
  content   String   // 投稿内容
  createdAt DateTime @default(now())
}
