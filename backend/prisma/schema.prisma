generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Fukuoka_water_info {
  id    Int     @id @default(autoincrement())
  prefecture     String
  prefecture_code String
  flood_level    String
  geometry       Json
}

