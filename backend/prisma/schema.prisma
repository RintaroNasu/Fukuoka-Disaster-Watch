generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Fukuoka_land_info {
  id    Int     @id @default(autoincrement())
  location_name     String
  address          String
  record_date    String
  geometry       Json
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  password  String
  comments  Comment[]
}

model Comment {
  id        Int      @id @default(autoincrement())
  lat       Float 
  lng       Float
  content   String
  userId   Int  
  createdAt DateTime @default(now())
  user      User     @relation(fields: [userId], references: [id])
}
